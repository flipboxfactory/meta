{
	"name": "flipboxfactory/meta",
	"description": "A simple, repeatable field type",
	"version": "1.0.0-rc.3",
	"type": "craft-plugin",
	"keywords": [
		"flipbox",
		"meta",
		"craftcms",
		"yii2"
	],
	"license": "MIT",
	"authors": [{
		"name": "Flipbox Factory",
		"email": "hello@flipboxfactory.com"
	}],
	"support": {
		"email": "support@flipboxfactory.com",
		"issues": "https://github.com/flipboxfactory/meta/issues?state=open",
		"source": "https://github.com/flipboxfactory/meta",
		"docs": "https://github.com/flipboxfactory/meta",
		"rss": "https://github.com/flipboxfactory/meta/commits/master.atom"
	},
	"require": {
		"craftcms/cms": "^3.0.0",
		"flipboxfactory/craft-ember": "^1.0",
		"flipboxfactory/craft-sortable-associations": "^1.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^5.0",
		"squizlabs/php_codesniffer": "^2.0"
	},
	"autoload": {
		"psr-4": {
			"flipbox\\meta\\": "src/"
		}
	},
	"extra": {
		"name": "Meta",
		"handle": "meta",
		"class": "flipbox\\meta\\Meta",
		"changelogUrl": "https://raw.githubusercontent.com/flipboxfactory/meta/master/CHANGELOG.md",
		"downloadUrl": "https://github.com/flipboxfactory/meta/archive/master.zip",
		"hasCpSettings": false,
		"hasCpSection": false,
		"schemaVersion": "1.1.0",
		"components": {
			"configuration": "flipbox\\meta\\services\\Configuration",
			"elements": "flipbox\\meta\\services\\Elements",
			"fields": "flipbox\\meta\\services\\Fields",
			"records": "flipbox\\meta\\services\\Records"
		}
	}
}
